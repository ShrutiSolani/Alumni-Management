create database alumniManagement;

drop table alumni;
create table alumni(
aId integer Auto_increment Primary Key,
full_name varchar(50),
dept varchar(20),
cur_comp varchar(50),
intern_comp varchar(50),
post varchar(30),
passedOutYear integer,
descr varchar(500),
email varchar(20),
platformLink varchar(30),
otherLink varchar(30)
);

